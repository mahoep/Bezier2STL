FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            flowRateInletVelocity;
        massFlowRate    constant 0.005;
        rho             rho;
        rhoInlet        1.225;
        value           uniform (0 0 0);
    }

    outlet
    {
        type            waveTransmissive;
        gamma           1.4;
    }

    symmetryLine
    {
        type            symmetryPlane;
    }

    walls
    {
        type            noSlip;
    }


    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
